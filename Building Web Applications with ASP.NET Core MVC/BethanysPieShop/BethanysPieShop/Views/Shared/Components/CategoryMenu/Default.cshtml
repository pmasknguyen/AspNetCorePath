@model IEnumerable<Category>
    <li class="dropdown">
        <a asp-controller="Pie" asp-action="Index" data-toggle="dropdown">
            Pies<b class="caret"></b>
        </a>
        <ul class="dropdown-menu">
            @foreach (var category in Model)
            {
                <li>
                    <a asp-controller="Pie" asp-action="List" asp-route-category="@category.CategoryName">@category.CategoryName</a>
                </li>
            }
            <li class="divider"></li>
            <li><a asp-controller="Pie" asp-action="List">All Pies</a></></li>
        </ul>
    </li>
